<Window x:Class="SparseMatrixCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SparseMatrixCalculator"
        mc:Ignorable="d"
        Title="Sparse Matrix Calculator" Height="450" Width="800" FontFamily="Segoe UI">
    <TabControl>
        <TabItem Header="Matrix A">
            <Frame x:Name="MatrixAPage" Source="MatrixInput.xaml" NavigationUIVisibility="Hidden"/>
        </TabItem>
        
        <TabItem Header="Matrix B">
            <Frame x:Name="MatrixBPage" Source="MatrixInput.xaml" NavigationUIVisibility="Hidden"/>
        </TabItem>
        
        <TabItem Header="Operation">
            
        </TabItem>
    </TabControl>
</Window>
